<table>
  <tr>
    <th>Title</th>
    <th>Author</th> 
    <th>ISBN</th>
  </tr>

  <% @books.each do |book| %>
    <tr>
      <td><%= book.title %></td>
      <td><%= book.author %></td>
      <td><%= book.ISBN %></td>
      <td><%= link_to 'Add Review', new_book_review_path(book) %></td>
      <td><%= link_to 'See Review', book_path(book) %></td>
      
    </tr>
  <% end %>
</table>

<%= link_to 'Back to search', patron_index_path %>